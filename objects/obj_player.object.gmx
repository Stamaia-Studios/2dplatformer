            <kind>1</kind>
            <string>/// Init Variables

grav = 0.2; // Gravity
hsp = 0; // Horizontal Speed
vsp = 0; // Vertical Speed
jumpspeed = 7; // Jump Speed Constant
movespeed = 4; // Movement Speed Constant
dash_left_timer = 0;
dash_right_timer = 0;
dash_timeout = 10;
dashtrigger = false;
</string>
          </argument>
        </arguments>
      </action>
