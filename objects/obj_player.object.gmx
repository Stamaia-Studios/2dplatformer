        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Controls

key_right = vk_right;
key_left = vk_left;
key_jump = ord('Z');
key_attack = ord('X');
key_down = vk_down;
key_run = vk_shift;
taptorun = false;
key_action = ord('C'); // pick_up and throw
</string>
